{
  "name": "sentry-sourcemaps",
  "version": "0.0.1",
  "description": "Upload your Javascript source maps to Sentry",
  "main": "dist/index",
  "repository": {
    "type": "git",
    "url": "https://github.com/polyconseil/sentry-sourcemaps"
  },
  "scripts": {
    "prepublish": "babel --presets es2015 --source-maps true src --out-dir dist/",
    "test": "mocha --compilers js:babel-core/register src/**/*.spec.js",
    "cover": "babel-node node_modules/isparta/bin/isparta cover --report text  node_modules/mocha/bin/_mocha -- src/**/*.spec.js"
  },
  "keywords": [],
  "author": "Polyconseil",
  "license": "MIT",
  "devDependencies": {
    "babel": "^6.1.18",
    "babel-cli": "^6.2.0",
    "babel-core": "^6.2.1",
    "babel-preset-es2015": "^6.1.18",
    "chai": "^3.4.1",
    "eslint": "^1.10.1",
    "isparta": "^4.0.0",
    "mocha": "^2.3.4"
  },
  "bin": {
    "sentry-upload": "./dist/cli.js"
  },
  "files": [
    "src",
    "dist",
    "package.json",
    "README.md"
  ],
  "dependencies": {
    "asyncawait": "^1.0.1",
    "bluebird": "^3.0.6",
    "npm-registry-client": "^7.0.9",
    "request": "^2.67.0",
    "tar.gz": "^1.0.2",
    "temp": "^0.8.3",
    "yargs": "^3.31.0"
  }
}
